{
    "name": "phossa/phossa-cache",
    "version": "1.0.0",
    "license": "MIT",
    "type": "library",
    "description": "The PSR-6 compliant caching package of Phossa project",
    "keywords": ["phossa", "cache", "psr-6"],
    "homepage": "https://github.com/phossa/phossa-cache",
    "authors": [
        {
            "name": "Hong Zhang",
            "homepage": "https://github.com/phossa"
        }
    ],
    "autoload": {
        "psr-0": { "Phossa\\Cache\\": "src/" }
